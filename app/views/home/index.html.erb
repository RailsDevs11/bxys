
<!--top row start-->
<div class="row top-row">
  <div class="social-media">
    <a href="#"><%= image_tag("facebook_icon.png")%></a>
    <a href="#"><%= image_tag("twitter.png")%></a>
  </div>
  <p>
    En başarlı müzik grubu hangisi?
    <span>(382 kez oylandı.)</span>
  </p>
  <ul>
    <li>
      Grup Hepsi
      <span>1.587</span>
    </li>
    <li>
      Sepultura
      <span>81</span>
    </li>
  </ul>
</div>
  <!--top row end-->
  
<!--middle row start-->
<div class="row middle-row">
  <%= render partial: "quize" %>
</div>
<!--middle row end-->

<!--bottom row start-->
<div class="row middle-row bottom-row">
  <div class="social-media social-media-second">
    <a href="#"><%= image_tag("facebook_icon.png")%></a>
    <a href="#"><%= image_tag("twitter.png")%></a>
  </div>
  <p>
    Hangi spor dalı?
    <span><small>(430 kez oylandı.)</small></span>
  </p>
  <div class="col-md-12 padding-none">
    <ul id="myTab" class="product-selection disable">
      <div class="or-div"></div>
      <li>
        <a data-toggle="tab" href="#home">
          <div class="choose-option"></div>
            <p>Futbol</p>
            <%= image_tag("product3.png")%>
        </a>
      </li>
      <li>
        <a data-toggle="tab" href="#profile">
          <div class="choose-option"></div>
            <p>Basketbol</p>
            <%= image_tag("product4.png")%>
        </a>
      </li>
    </ul>
  </div>
</div>
  <!--bottom row end-->



